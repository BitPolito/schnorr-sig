{
   "$schema": "http://json-schema.org/draft-07/schema",
   "title": "JSON Schema for multiple keypairs",
   "definitions": {
      "keypair": {
         "type": "object",
         "properties": {
            "publicKey": { "type": "string" },
            "privateKey": { "type": "string" }
         }
      }
   },
   
   "type": "object",
   "properties": {
      "keypairs": { 
         "type": "array",
         "keypair": { "$ref": "#definitions/keypair" }
      }
   }
}